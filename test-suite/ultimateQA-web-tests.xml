<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test Suite | Project QA Automation"  parallel="tests" thread-count="2" verbose="1">

    <listeners>
        <listener class-name="components.TestListener"/>
    </listeners>
    <parameter name="WEB_URL" value="https://ultimateqa.com/automation"/>
